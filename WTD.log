      var temp = store;
      var v = "";
      var ivar = temp.indexOf(sepvar + variable + sepval);
      if (ivar >= 0)
         {
         temp = temp.substring(ivar + sepvar.length, temp.length);
         var v_array=temp.split(sepval);
         v = v_array[1];               
         }
      return(v);
      }
   }

